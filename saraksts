s0 = []
for i in range ( 5 ):
    x = int(input("Enter x:"))
    s0.append(x)
print(s0)
    
for item in s0:
    item *=2
    print(item)
    
for i in range (0, len(s0)):
    s0[i] *=100
    print(s0)
